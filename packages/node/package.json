{
  "name": "@crypax/node",
  "version": "0.2.0",
  "description": "Crypax server-side SDK for Node.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikusnuz/crypax-sdk.git",
    "directory": "packages/node"
  },
  "homepage": "https://github.com/mikusnuz/crypax-sdk/tree/main/packages/node",
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["crypax", "payment", "crypto", "node", "server", "sdk"],
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  }
}
