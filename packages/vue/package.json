{
  "name": "@crypax/vue",
  "version": "0.2.0",
  "description": "Crypax Vue SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikusnuz/crypax-sdk.git",
    "directory": "packages/vue"
  },
  "homepage": "https://github.com/mikusnuz/crypax-sdk/tree/main/packages/vue",
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["crypax", "payment", "crypto", "vue", "sdk"],
  "dependencies": {
    "@crypax/js": "workspace:*"
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "devDependencies": {
    "vue": "^3.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  }
}
