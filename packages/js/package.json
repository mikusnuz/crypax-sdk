{
  "name": "@crypax/js",
  "version": "0.5.1",
  "description": "Crypax payment SDK for browsers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikusnuz/crypax-sdk.git",
    "directory": "packages/js"
  },
  "homepage": "https://github.com/mikusnuz/crypax-sdk/tree/main/packages/js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "crypax",
    "payment",
    "crypto",
    "blockchain",
    "sdk"
  ],
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  }
}
